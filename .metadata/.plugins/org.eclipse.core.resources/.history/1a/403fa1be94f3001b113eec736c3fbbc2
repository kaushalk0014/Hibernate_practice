    public static List<Customer> getAll(){
    	return Stream.of(
    			new Customer(101, "kaushal", "kaushal@gmail.com", Arrays.asList("4257854258","7584512569")),
    			new Customer(101, "kaushal", "kaushal@gmail.com", Arrays.asList("4257854258","7584512569")),
    			new Customer(101, "kaushal", "kaushal@gmail.com", Arrays.asList("4257854258","7584512569"))   				
    		);
    }
}
